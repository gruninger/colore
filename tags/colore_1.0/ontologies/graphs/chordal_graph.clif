
(cl-text chordal_graph

(cl-imports undirected_graph)

(forall (x y z w)
	(if	(cycle x y z w ...)
		(exists (u v)
			(and	(path x ... u ... w)
				(path x ... v ... w)
				(adj u v)))))

(forall (x y z)
	(iff	(path x ... y z)
		(and	(path x ... y)
			(adj y z))))

(forall (x y)
	(iff	(cycle x ... y)
		(and	(path x ... y)
			(adj x y))))



)
