
(cl-text path_graph

(cl-imports undirected_graph)

(exists (x y)
	(and	(path-endpoint x)
		(path-endpoint y)
		(forall (z)
			(if	(path-endpoint z)
				(or	(= z x)
					(= z y))))))

(forall (x)
	(if	(not (path-endpoint x))
		(exists (y z)
			(and	(adj x y)
				(adj x z)
				(forall (w)
					(if	(adj x w)
						(or	(= w y)
							(= w z))))))))

(forall (x)
	(iff	(path-endpoint x)
		(exists (y)
			(and	(adj x y)
				(forall (z)
					(if	(adj x z)
						(= y z)))))))


)
