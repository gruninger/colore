
(cl-text boxworld_surface

(cl-imports boxworld_edge)

(cl-comment "Each point, edge, and surface is part of a unique box.")
(forall (x1 x2 y)
        (if     (and    (part y x1) (part y x2) (box x1) (box x2)
                        (or (point y) (edge y) (surface y)))
                (= x1 x2)))
)

