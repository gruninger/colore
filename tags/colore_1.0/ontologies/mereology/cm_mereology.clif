
(cl-text cem_mereology

(cl-imports em_mereology)

(cl-comment "Sum")
(forall (x y)
	(if	(underlaps x y)
		(exists (z)
			(forall (v)
				(iff	(overlaps v z)
					(or	(overlaps v x)
						(overlaps v y)))))))

(cl-comment "Product")
(forall (x y)
	(if	(overlaps x y)
		(exists (z)
			(forall (v)
				(iff	(ppart v z)
					(and	(ppart v x)
						(ppart v y)))))))

)
