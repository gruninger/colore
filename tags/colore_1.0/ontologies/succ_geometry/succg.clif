
(cl-text succg

(cl-imports weak_bipartite)

(cl-imports successor)

(forall (x y)
	(if	(S x y)
		(and	(point x)
			(point y))))

(forall (x y)
	(iff	(or	(S x y)
			(S y x))
		(exists (l)
			(and	(line l)
				(in x l)
				(in y l)))))

(forall (x)
	(if	(point x)
		(exists (y)
			(S x y))))

(forall (x)
	(if	(point x)
		(exists (y)
			(S y x))))

)
