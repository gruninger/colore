(cl-text uni_complemented_lattice

(cl-imports complemented_lattice)

(cl-comment "Uniquely complemented lattice axioms")

(cl-comment "UC1: Complements are unique")

(forall (x y z)
	(if 
		(and 	(complement x z)
			(complement y z))
		(= x y)
))

)
