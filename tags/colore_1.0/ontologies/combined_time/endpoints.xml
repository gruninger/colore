<?xml version="1.0" encoding="UTF-8"?>

<?xml-stylesheet type="text/xsl" href="/colore/COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:time="http://stl.mie.utoronto.ca/Colore/time/" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema">

	<moduleDefinition module="endpoints" axiomFile="endpoints.clif" />

	<head>
		<version>1.0</version>
		<date>2010-02-26</date>
		<description>Axioms for Linear Timepoints and Intervals</description>
		<keyword>timepoint</keyword>
		<keyword>time interval</keyword>
		<keyword>linear ordering</keyword>
                <source>
                        <name>Catalog of Temporal Theories</name>
                        <author>Pat Hayes</author>
                        <year>1996</year>
                </source>
		<author>Michael Gruninger</author>
		<parentOntology/>
