		<definedLexicalSymbol symbol="overlaps" type="relation" arity="2"/>
		<definedLexicalSymbol symbol="starts" type="relation" arity="2"/>
		<definedLexicalSymbol symbol="during" type="relation" arity="2"/>
		<definedLexicalSymbol symbol="finishes" type="relation" arity="2"/>
	</lexicon>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="timepoint" type="relation" arity="1"/>
		<primitiveLexicalSymbol symbol="before" type="relation" arity="2"/>
	</inheritedLexicon>

	<directRelations>
