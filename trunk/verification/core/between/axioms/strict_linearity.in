
formulas(assumptions).

(all x all y all z 
	(between(x,y,z)
	->
	between(z,y,x))).

(all x all y
	(between(x,y,x)
	->
	(x=y))).

(all x all y all z
	((between(x,y,z)
	& between(y,x,z))
	->
	(x=y))).

(all x all y all z
	(((x != y)
	& (x != z)
	& (y != z))
	->
	(between(x,y,z) | between(y,z,x) | between(z,x,y)))).

end_of_list.
