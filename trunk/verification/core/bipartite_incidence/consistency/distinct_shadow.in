
formulas(assumptions).

all x all y
	(in(x,y)
	->
	in(y,x)).

all x
	(in(x,x)).

all p
	(point(p)
	->
	-line(p)).

all x all y
	((in(x,y)
	& point(x)
	& point(y))
	->
	(x=y)).

all x all y
	((in(x,y)
	& line(x)
	& line(y))
	->
	(x=y)).

%distinct_shadow

(all x all y
	((line(x)
	& line(y)
	& (x != y))
	->
	(exists z
		(point(z)
		& in(z,x)
		& -in(z,y))))).

(exists p exists l
        (point(p)
        & line(l)
        & in(p,l))).


end_of_list.
