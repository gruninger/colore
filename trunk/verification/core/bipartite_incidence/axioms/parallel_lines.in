
formulas(assumptions).

all x all y
	(in(x,y)
	->
	in(y,x)).

all x
	(in(x,x)).

all p
	(point(p)
	->
	-line(p)).

all x all y
	((in(x,y)
	& point(x)
	& point(y))
	->
	(x=y)).

all x all y
	((in(x,y)
	& line(x)
	& line(y))
	->
	(x=y)).

%parallel_lines

(all l1 all l2 all p
	((line(l1)
	& line(l2)
	& point(p)
	& in(p,l1)
	& in(p,l2))
	->
	(l1=l2))).

end_of_list.
