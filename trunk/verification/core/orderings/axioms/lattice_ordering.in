
formulas(assumptions).

% Reflexive
all x
	(leq(x,x)).

% Antisymmetry
all x all y
	((leq(x,y)
	& leq(y,x))
	->
	(x=y)).

% Transitive
all x all y all z
	((leq(x,y)
	& leq(y,z))
	->
	leq(x,z)).

(all x all y
	(exists m
		(leq(m,x)
		& leq(m,y)
		& (all z
			((leq(z,x)
			& leq(z,y))
			->
			leq(z,m)))))).

(all x all y
	(exists j
		(leq(x,j)
		& leq(y,j)
		& (all z
			((leq(x,z)
			& leq(y,z))
			->
			leq(j,z)))))).

end_of_list.
