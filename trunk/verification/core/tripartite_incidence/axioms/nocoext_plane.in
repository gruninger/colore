
formulas(assumptions).

all x all y
	(in(x,y)
	->
	in(y,x)).

all x
	(in(x,x)).

all p
	(point(p)
	->
	-line(p)).

all p
	(point(p)
	->
	-plane(p)).

all p
	(line(p)
	->
	-plane(p)).

all x all y
	((in(x,y)
	& point(x)
	& point(y))
	->
	(x=y)).

all x all y
	((in(x,y)
	& line(x)
	& line(y))
	->
	(x=y)).

all x all y
	((in(x,y)
	& plane(x)
	& plane(y))
	->
	(x=y)).

(all q1 all q2
	((plane(q1)
	& plane(q2)
	& (q1 != q2))
	->
	(exists l
		(line(l)
		& in(l,q1)
		& -in(l,q2))))).

end_of_list.
