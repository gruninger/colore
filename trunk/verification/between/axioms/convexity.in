
formulas(assumptions).

(all x all y all z 
	(between(x,y,z)
	->
	between(z,y,x))).

(all x all y
	(between(x,y,x)
	->
	(x=y))).

(all x all y all z
	((between(x,y,z)
	& between(y,x,z))
	->
	(x=y))).

(all x all y all z all w all v
	((between(x,y,z)
	& between(x,w,z)
	& between(y,v,w))
	->
	between(x,v,z))).

end_of_list.
