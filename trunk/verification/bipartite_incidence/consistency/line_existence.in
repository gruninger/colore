
formulas(assumptions).

all x all y
	(in(x,y)
	->
	in(y,x)).

all x
	(in(x,x)).

all p
	(point(p)
	->
	-line(p)).

all x all y
	((in(x,y)
	& point(x)
	& point(y))
	->
	(x=y)).

all x all y
	((in(x,y)
	& line(x)
	& line(y))
	->
	(x=y)).

%line_existence

all x all y
	((point(x)
	& point(y)
	& (x != y))
	->
	(exists z
		(line(z)
		& in(x,z)
		& in(y,z)))).

(exists p exists l
        (point(p)
        & line(l)
        & in(p,l))).


end_of_list.
