
formulas(assumptions).

% Reflexive
all x
	(leq(x,x)).

% Antisymmetry
all x all y
	((leq(x,y)
	& leq(y,x))
	->
	(x=y)).

% Transitive
all x all y all z
	((leq(x,y)
	& leq(y,z))
	->
	leq(x,z)).

% Complete
all x all y
        ((x=y)
        | leq(x,y)
        | leq(y,x)).

(exists x
        (all y
                (-lt(y,x)))).

(exists x
        (all y
                (-lt(x,y)))).


end_of_list.
