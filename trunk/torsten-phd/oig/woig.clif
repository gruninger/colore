(cl-module oig/woig_3d

(cl-imports oig/b_3d)

(cl-imports ig/ig)

(cl-comment '3D weak ordered incidence geometry')

		
(cl-comment 'only points in betweenness: Provable')

(forall (a b c)
	(if
		(B a b c)
		(Pt a)
	)
)

(forall (a b c)
	(if
		(B a b c)
		(Pt b)
	)
)

(forall (a b c)
	(if
		(B a b c)
		(Pt c)
	)
)

)