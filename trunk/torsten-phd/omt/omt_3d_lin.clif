(cl-module omt/omt_3d_lin

(cl-imports omt/omt_down_plp_lin)


(cl-comment 'OMT-E1')

(forall (x y z l)
	(if
		(and
			(Pt x)
			(Pt y)
			(Pt z)
			(L l)
			(Cont x l)
			(Cont y l)
			(Cont z l)
			(not (C x y))
			(not (C x z))
			(not (C y z))
		)
		(or
			(Btw l x y z)
			(Btw l y z x)
			(Btw l z x y)
		)
	)
)

(cl-comment 'OMT-E2')

(forall (x)
	(or
		(Pt x)
		(L x)
		(Pl x)
		(ZEX x)
	)
)
			
)