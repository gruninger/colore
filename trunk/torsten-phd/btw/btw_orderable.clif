(cl-module btw/btw_orderable

(cl-imports btw/btw_basic)

(cl-comment 'axioms of orderable betweenness')

(cl-comment 'B: one of three distinct elements is in the middle position')

(forall (r a b c)
	(if
		(and
			(not (= a b))
			(not (= a c))
			(not (= b c))
		)
		(or
			(Btw r a b c)
			(Btw r a c b)
			(Btw r b a c)
		)
	)
)

			
)