(cl-module btw/btw_basic_theorems

(cl-imports btw/btw_basic)


(cl-comment 'B-T1: ')

(forall (r a b c)
	(if
		(Btw r a b c)
		(not (Btw r b a c))
	)
)


(cl-comment 'B-T2: Huntington postulate 3')

(forall (r a b x y)
	(if
		(and
			(Btw r x a b)
			(Btw r a y b)
		)
		(Btw r x y b)
	)
)



(cl-comment 'B-A6: separation property: NOT PROVABLE')

(forall (r a b c x)
	(if
		(and
			(Btw r a x b)
			(Btw r a x c)
		)
		(not (Btw r b x c))
	)
)


)