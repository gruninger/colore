(cl-text mt/rcc_full

(cl-imports mt/rcc_basic)

(cl-comment "RCC4a")

(forall (x y)
	(if
		(not (= y Uni))
		(iff	
			(C x (compl y))
			(not (NTPP x y))
		)	
	)			
)

(cl-comment "RCC8")

(forall (x)
	(exists (y)
		(NTPP y x)
	)
)


)
