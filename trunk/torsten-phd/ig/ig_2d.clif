(cl-module ig/ig_2d

(cl-comment 'axioms of line spaces')

(cl-comment 'I.0a')

(forall (x y)
	(if
		(on x y)
		(and
			(Pt x)
			(not (Pt y))
		)
	)
)


(cl-comment 'I.1')

(forall (x)
	(if
		(L x)
		(exists (y z)
			(and
				(on y x)
				(on z x)
				(not (= y z))
			)
		)
	)
)
		

)