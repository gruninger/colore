(cl-text inch/codi_linear_I-PA3

(cl-imports codi/codi_linear)
(cl-imports zex/zex)
(cl-imports inch/inch_mapping)

(cl-comment "I-PA3: reflexivity of INCH")

(forall (x y)
	(if
		(INCH x y)
		(INCH x x)
	)
)


)