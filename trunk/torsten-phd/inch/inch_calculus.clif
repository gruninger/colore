(cl-module inch/inch_calculus

(cl-imports inch/inch_original)

(cl-comment 'corrected INCH Calculus with only the essential definitions')

(cl-comment 'I-PA7*: INCH(x,y) requires a chunk of x to overlap with y')

(forall (x y)
	(if
		(INCH x y)
		(exists (z)
			(and
				(CS z x)
				(CH z y)
			)
		)
	)
)

)