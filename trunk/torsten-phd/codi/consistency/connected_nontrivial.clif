(cl-text  codi/consistency/connected_nontrivial

(cl-imports  codi/defs/connected)


(exists (x)
	(and
		(Con x)
		(Min x)
	)
)

(exists (x)
	(and
		(Con x)
		(not (ZEX x))
		(not (Min x))
	)
)



(exists (x)
	(and
		(not (Con x))
		(not (Min x))
	)
)


)
