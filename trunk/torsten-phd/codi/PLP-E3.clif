(cl-module codi/PLP-E3

(cl-comment 'PLP-E3: Plane Dimension Axiom')

(exists (p q r s)
	(and
		(Pt p)
		(Pt q)
		(Pt r)
		(Pt s)
		(not (= p q))
		(not (= p r))
		(not (= p s))
		(not (= q r))
		(not (= q s))	
		(not (= r s))
		(forall (x)
			(if
				(Pl x)
				(or
					(not (Cont p x))
					(not (Cont q x))
					(not (Cont r x))
					(not (Cont s x))
				)
			)
		)
	)
)

)
