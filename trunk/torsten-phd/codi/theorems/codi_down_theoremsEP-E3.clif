(cl-module codi/theorems/codi_down_theoremsEP-E3

(cl-imports codi/theorems/codi_down_theoremsEP-E2)


(cl-comment 'EP-E3: strong supplementation for containment')

(forall (x y)
	(if
		(and
			(not (ZEX x))
			(not (ZEX y))
			(not (Cont y x))
		)
		(exists (z)
			(and
				(P z y)
				(< (intersection z x) z)
			)
		)
	)
)

)