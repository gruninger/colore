(cl-module codi/theorems/codi_plp_theorems

(cl-imports codi/codi_plp)


(cl-comment 'PLP-T1')

(forall (x y)
	(if
		(and
			(Pl x)
			(Pl y)
		)
		(EqDim x y)
	)
)

(cl-comment 'PLP-T2a')

(forall (x)
	(or
		(not (Pt x))
		(not (Pl x))
	)
)

(cl-comment 'PLP-T2b')

(forall (x)
	(or
		(not (L x))
		(not (Pl x))
	)
)


)