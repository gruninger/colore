(cl-module  codib/consistency/codib_updown_nontrivial_simple

(cl-imports  codib/codib_updown)

(cl-imports codi/defs/epp)

(cl-imports codi/defs/connected)

(exists (x y z r s t v)
	(and
		(BCont x y)
		(Cont x z)
		(not (BCont x z))
		(SC y z)
		(not (Con r))
		(PP t r)
		(PP s r)
		(= (difference r t) s)
		(BCont v t)
		(BCont v x)
		(Inc t x)
		(< t x) 
	)
)


)
