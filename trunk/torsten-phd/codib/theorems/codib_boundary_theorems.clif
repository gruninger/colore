(cl-module codib/codib_boundary_theorems

(cl-imports codib/codib_boundary)


(cl-comment 'BC-T8: boundary is boundary contained')

(forall (x)
	(if
		(not (Closed x))
		(BCont (boundary x) x)
	)
) 

(cl-comment 'BC-T9: zero boundary of closed entities')

(forall (x)
	(if
		(Closed x)
		(ZEX (boundary x))
	)
)

)