(cl-module codib/codib_icont_tcont_theorems

(cl-imports codib/codib)

(cl-imports codib/defs/icont)

(cl-imports codib/defs/tcont)


(cl-comment 'BC-T6: ICont and TCont disjoint')

(forall (x y)
	(or
		(not (ICont x y))
		(not (TCont x y))
	)
)


(cl-comment 'BC-T7: ICont and TCont exhaustive w.r.t. Cont') 

(forall (x y)
	(if
		(Cont x y)
		(or
			(ICont x y)
			(TCont x y)
		)
	)
)

)