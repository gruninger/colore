(cl-module codib/codib_down

(cl-imports codib/codib)

(cl-imports codi/codi_down)

(cl-imports codi/codi_basic_atomic)

(cl-imports codib/defs/icont)

(cl-imports codib/defs/tcont)


(cl-comment "BC-A6")

(forall (x y)
	(iff
		(BCont x y)
		(and
			(not (ZEX x))
			(forall (z)
				(if
					(and
						(P z x)
						(Min z)
					)
					(BCont z y)
				)
			)
		)
	)
)
				
)