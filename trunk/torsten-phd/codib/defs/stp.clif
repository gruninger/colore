(cl-module codib/defs/stp

(cl-imports codib/defs/tp)

(cl-imports codib/codib_boundary)

(cl-comment "Defining strong tangential parthood")

(cl-comment "STP-D")

(forall (x y)
	(iff
		(STP x y)
		(and
			(TP x y)
			(exists (z)
				(and
					(Cont z x)
					(BCont z y)
					(Covers y z)
				)
			)
		)
	)
)

)