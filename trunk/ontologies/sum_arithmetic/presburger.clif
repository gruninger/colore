(cl-text http://colore.oor.net/sum_arithmetic/presburger.clif

(forall (x)
	(not (= zero (plus x one))))

(forall (x y)
	(if	(= (plus x one) (plus y one))
		(= x y)))

(forall (x)
	(= x (plus x zero)))

(forall (x y)
	(= (plus (plus x y) one) (plus x (plus y one))))

(forall (p)
	(if	(and	(p zero)
			(forall (x)
				(if	(p x)
					(p (plus x one)))))
		(forall (y)
			(p y))))

)
