
(cl-text http://colore.oor.net/subposet/subposet_root

(cl-module Q (cl-imports http://colore.oor.net/mereology/m_mereology))

(cl-module P (cl-imports http://colore.oor.net/orderings/partial_ordering))

(forall (x y)
	(if	(part x y)
		(and	(Q x)
			(Q y))))

(forall (x y)
	(if	(leq x y)
		(and	(P x)
			(P y))))

)
