(cl-text moment

(forall (x y)
	(if	(pre x y)
		(and	(mo x)
			(mo y))))

(forall (x y z)
	(if	(and	(pre x y)
			(pre y z))
		(pre x z)))

(forall (x)
	(not (pre x x)))

(forall (x y z)
	(if	(and	(pre x z)
			(pre y z))
		(or	(= x y)
			(pre x y)
			(pre y x))))

(forall (x y)
	(if	(and	(mo x)
			(mo y))
		(exists (z)
			(and	(or	(pre z x)
					(= z x))
				(or	(pre z y)
					(= z y))))))

(exists (x)
	(mo x))

)

