(cl-text ladkin_intq

(cl-imports allen_hayes)

(forall (p q r s)
	(iff	(equiv p q r s)
			(and	(meets p q)
					(meets r s)
					(meets p s))))

(forall (p q r s)
	(iff	(pointless p q r s)
			(exists (u v w)
				(and	(equiv p q u v)
						(equiv r s v w)))))
(forall (p q r s)
(if	(pointless p q r s)
	(exists (x y)
		(and	(pointless p q x y)
			(pointless x y r s)))))
)
