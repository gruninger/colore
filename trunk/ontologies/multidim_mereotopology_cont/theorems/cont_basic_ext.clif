(cl-text multidim_mereotopology_cont/theorems/cont_basic_ext.clif

(cl-imports multidim_mereotopology_cont/cont_basic.clif)

(cl-comment "C-T1: Cont is extensional")


(forall (x y)
	(if
		(forall (z)
			(iff
				(Cont z x)
				(Cont z y)
			)
		)
		(= x y)
	)
)



)
