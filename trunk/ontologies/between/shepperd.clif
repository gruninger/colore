(cl-text http://colore.oor.net/between/shepperd.clif

(forall (x y z)
	(if	(between x y z)
		(between z y x)))

(forall (x y z)
	(or	(between x y z)
		(between y z x)
		(between z x y)))

(forall (x y z)
	(iff	(and	(between x y z)
			(between x z y))
		(= y z)))

(forall (x y z w)
	(if	(and	(between x y z)
			(between x z w))
		(between y z w)))

)

