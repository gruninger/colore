
(cl-text http://colore.oor.net/between/semi_fishburn

(cl-imports http://colore.oor.net/between/fishburn)

(forall (x y z a b c)
	(if	(and	(between a b c)
			(between x y z))
		(or	(between a b x)
			(between a b z)
			(between x b c)
			(between z b c))))
				
(forall (x a b c d)
	(if	(and	(between a b c)
			(between b c d)
			(not	(between a b x)))
		(or	(between x c d)
			(between a x d))))

)
