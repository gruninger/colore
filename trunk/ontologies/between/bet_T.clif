
(cl-text http://colore.oor.net/between/bet_T.clif

(cl-imports http://colore.oor.net/between/betweenness.clif)

(forall (x y z w)
        (if     (and    (between x y z)
                        (between y z w))
                (between x y w)))

(forall (x y z w)
        (if     (and    (between x y w)
                        (between y z w))
                (between x y z)))

