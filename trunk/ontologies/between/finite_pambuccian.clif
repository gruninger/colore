
(cl-text http://colore.oor.net/between/finite_pambuccian.clif

(forall (a b c)
	(if	(between a b c)
		(between c b a)))

(forall (a b c)
	(if	(between a b c)
		(not (between a c b))))

(forall (a b c d)
	(if	(and	(between a c b)
			(between a b d))
		(between c b d)))

(forall (a b c d)
	(if	(and	(between c a b)
			(between a b d))
		(between c b d)))

(forall (a b c d)
	(if	(and	(between a b c)
			(between a b d)
			(not (= c d)))
		(or	(between c b d)
			(between b d c))))

	

)
