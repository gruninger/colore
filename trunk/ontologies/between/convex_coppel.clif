
(cl-text http://colore.oor.net/between/convex_coppel.clif

(cl-imports http://colore.oor.net/between/betweenness.clif)

(cl-comment "Axiom C")

(forall (a b1 b2 c d)
	(if	(and	(between a c b1)
			(between c d b2))
		(exists (b)
			(and	(between b1 b b2)
				(between a d b)))))

(cl-comment "Axiom L1")

(forall (a b)
	(if	(between a b a)
		(= b a)))

)

