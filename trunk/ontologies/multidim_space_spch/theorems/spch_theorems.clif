(cl-text http://colore.oor.net/multidim_space_spch/theorems/spch_theorems.clif

(cl-imports http://colore.oor.net/multidim_space_space/spch.clif)


(cl-comment "CH-T1")

(forall (x)
	(if
		(MaxDim x)
		(= (ch x) (ch (ch x)))
	)
)

(cl-comment "CH-T2")

(forall (x)
	(if
		(not (ICon (r x)))
		(not (ZEX x))
	)
)

(forall (x)
	(if
		(not (ZEX x))
		(ICon (ch x))
	)
)


(forall (x y)
	(if
		(and
			(not (ICon x))
			(ICon y)
		)
		(not (= x y))
	)
)


(cl-comment "CH-T3")

(= (ch Uni) Uni)


(cl-comment "CH-T4")

(forall (x)
	(if
		(MaxDim (r x))
		(EqDim (r x) (ch x))
	)
)


)