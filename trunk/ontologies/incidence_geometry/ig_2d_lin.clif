(cl-text http://colore.oor.net/incidence_geometry/ig_2d_lin

(cl-comment "linear line spaces")

(cl-imports http://colore.oor.net/incidence_geometry/ig_2d_slin)

(cl-comment "I.2b")

(forall (x y)
	(if
		(and
			(Pt x)
			(Pt y)
			(not (= x y))
		)
		(exists (z)
			(and
				(L z)
				(on x z)
				(on y z)
			)
		)
	)
)

)