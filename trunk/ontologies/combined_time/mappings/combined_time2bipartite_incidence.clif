
(cl-text combined_time2bipartite_incidence

(forall (x)
	(iff	(point x)
		(timepoint x)))

(forall (x)
	(iff	(line x)
		(timeinterval x)))

(forall (x y)
	(iff	(in x y)
		(or	(= (begins x) y)
			(= (ends x) y)
			(= (begins y) x)
			(= (ends y) x)
			(exists z
				(= (between x z) y))
			(exists z
				(= (between z x) y))
			(exists z
				(= (between y z) x))
			(exists z
				(= (between z y) x))
			(= x y))))

)
