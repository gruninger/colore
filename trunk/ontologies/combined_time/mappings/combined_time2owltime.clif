
(cl-text combined_time2owltime

(forall (x)
	(iff	(Instant x)
		(timepoint x)))

(forall (x)
	(iff	(Interval x)
		(timeinterval x)))

(forall (x)
	(iff	(TemporalEntity x)
		(or	(timepoint x)
			(timeinterval x))))

(forall (x y)
	(iff	(begins x y)
		(and	(= x (beginof y))
			(timepoint x)
			(timeinterval y))))

(forall (x y)
	(iff	(ends x y)
		(and	(= x (endof y))
			(timepoint x)
			(timeinterval y))))

)
