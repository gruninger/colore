
(cl-text discrete_partial

(cl-imports partial_ordering)

(cl-comment "Downwards discreteness")
(forall (x y)
	(if	(lt x y)
		(exists (z)
			(and	(covers y z)
				(leq x z)))))

(cl-comment "Upwards discreteness")
(forall (x y)
	(if	(lt x y)
		(exists (z)
			(and	(covers z x)
				(leq z y)))))

(forall (x y)
(iff	(covers y x)
	(and	(lt x y)
		(not (exists (z)
			(and	(lt x z)
				(lt z y)))))))

)
