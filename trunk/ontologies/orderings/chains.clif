
(cl-text http://colore.oor.net/orderings/chains.clif

(cl-imports http://colore.oor.net/orderings/partial_ordering.clif)

(forall (x y z)
	(if	(and	(leq x y)
			(leq x z))
		(or	(leq y z)
			(leq z y)
			(= x y))))

(forall (x y z)
	(if	(and	(leq x y)
			(leq z y))
		(or	(leq x z)
			(leq z x)
			(= x z))))

)
