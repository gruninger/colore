
(cl-text partial_ordering

(forall (x)
	(leq x x))

(forall (x y)
        (if     (and	(leq x y)
			(leq y x))
		(= x y)))

(forall (x y z)
        (if     (and    (leq x y)
                        (leq y z))
                (leq x z)))

(forall (x y)
(iff	(lt x y)
	(and	(leq x y)
		(not (leq y x)))))


)
