(cl-text http://colore.oor.net/approximate_point/ordered_ap.clif

(cl-imports http://colore.oor.net/approximate_point/approximate_point.clif)

(cl-comment "Glb")
(forall (x y z)
(iff	(glb x y z)
	(and	(finer z x)
		(finer z y)
		(forall (u)
			(if	(and	(finer u x)
					(finer u y))
				(finer u z))))))

(cl-comment "CONJ")
(forall	(x y)
(if	(ncdf x y)
	(exists	(z)
		(glb x y z))))

)
