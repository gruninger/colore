
(cl-text approximate_discrete_point

(cl-imports approximate_point)

(forall (x y)
(iff	(meets x y)
	(and	(precedes x y)
		(not (exists (z)
			(and	(precedes x z)
				(precedes z y)))))))

(forall (x)
	(exists (y)
		(and	(meets x y)
			(moment y))))

(forall (x)
	(exists (y)
		(and	(meets y x)
			(moment y))))

)

