(cl-text http://colore.oor.net/approximate_point/m_exist

(cl-imports http://colore.oor.net/approximate_point/approximate_point)

(cl-imports http://colore.oor.net/approximate_point/ap_def)

(forall (i j)
(if	(not	(meets i j))
	(or	(exists (x)
			(and	(meets i x)
				(meets x j)))
		(exists (y)
			(and	(meets j y)
				(meets y i))))))
)
