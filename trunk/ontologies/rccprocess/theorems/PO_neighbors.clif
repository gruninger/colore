(cl-text http://colore.oor.net/rcc_continuous_process/theorems/PO_neighbors.clif

(cl-imports http://colore.oor.net/rcc_continuous_process/rcc_continuous_process.clif)


(forall (o x y)  
		(if 	(changes o (POs x y)) 
				(or		(changes o (ECs x y))
						(changes o (TPPs x y))
						(changes o (TPPs y x)))))
	 
)
