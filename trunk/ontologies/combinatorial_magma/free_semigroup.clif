
(cl-text http://colore.oor.net/combinatorial_magma/free_semigroup.clif

(forall (x y z)
	(= (op x (op y z)) (op (op x y) z)))

(forall (x y z w)
	(if	(= (op x y) (op z w))
		(or	(and	(= x z)
				(= y w))
			(exists (u)
				(or	(and	(= z (op x u))
						(= y (op u w)))
					(and	(= x (op z u))
						(= w (op u y))))))))

(forall (x)
	(exists (y z)
		(and	(generator y)
			(= x (op y z)))))

)
