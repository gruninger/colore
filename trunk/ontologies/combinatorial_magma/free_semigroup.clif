
(cl-text http://colore.oor.net/combinatorial_magma/free_semigroup.clif

(cl-imports http://colore.oor.net/combinatorial_magma/tame_theoryF.clif)

(cl-imports http://colore.oor.net/combinatorial_magma/thin_TC.clif)

(cl-imports http://colore.oor.net/combinatorial_magma/definitions/words_def.clif)

(forall (x y)
	(if	(subword x y)
		(exists (z)
			(and	(initial_subword z y)
				(not (subword x z))
				(initial_subword (op z x) y)))))

)
