
(cl-text http://colore.oor.net/combinatorial_magma/theoryF.clif

(cl-imports http://colore.oor.net/magma/semigroup.clif)

(forall (x y z)
	(if	(= (op x y) (op x z))
		(= y z)))

(forall (x y z)
	(if	(= (op x y) (op x z))
		(= y z)))

(forall (x y z)
	(if	(= (op x z) (op y z))
		(= x y)))

(forall (x y z)
	(if	(and	(generator y)
			(generator z)
			(= (op x y) (op x z)))
		(= y z)))

(forall (z)
	(or	(generator z)
		(exists (x y)
			(and	(generator y)
				(= z (op x y))))))

)
