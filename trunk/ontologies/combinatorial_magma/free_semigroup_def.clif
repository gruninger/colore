
(cl-text free_semigroup_def

(cl-imports free_semigroup)

(forall (x y)
	(iff	(subword x y)
		(exists (u v)
			(= y (op u (op x v))))))

(forall (x y)
	(iff	(initial_subword x y)
		(exists (u)
			(= y (op x u)))))

(forall (x y)
	(iff	(end_subword x y)
		(exists (u)
			(= y (op u x)))))

(forall (x y z)
	(iff	(= z (append x y))
		(and	(generator x)
			(= z (op y x)))))

)
