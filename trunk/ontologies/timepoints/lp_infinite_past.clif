(cl-text http://colore.oor.net/timepoints/lp_infinite_past.clif

(cl-imports http://colore.oor.net/timepoints/lp_ordering.clif)

(forall (x)
	(if	(timepoint x) 
		(exists (y)
			(and	(timepoint y)
				(before y x)))))

)
