
(cl-text http://colore.oor.net/combinatorial_semigroup/monoid_theoryF.clif

(cl-imports http://colore.oor.net/magma/monoid.clif)

(cl-imports http://colore.oor.net/magma/cancellative.clif)

(forall (x y z w)
	(if	(and	(generator x)
			(generator y))
		(not (= (op w x) (op z y)))))

(forall (x)
	(if	(not (identity x))
		(exists (u y)
			(and	(generator y)
				(= (op u y) x)))))

)
