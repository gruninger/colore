(cl-text http://colore.oor.net/inch/consistency/inch_calculus_extended_full.clif

(cl-imports http://colore.oor.net/inch/inch_calculus_extended_full.clif)

(exists (x y z)
	(and
		(INCH x z)
		(not (INCH z x))
		(INCH x y)
		(not (= x y))
		(GED x y)
		(GED y x)
		(GED x z)
		(not (GED z x))
	)
)

)
