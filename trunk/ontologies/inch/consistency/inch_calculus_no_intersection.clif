(cl-text http://colore.oor.net/inch/consistency/inch_calculus_no_intersection.clif

(cl-imports http://colore.oor.net/inch/inch_calculus.clif)


(exists (x y r s)
	(and
		(not (= r x))
		(not (= s x))
		(not (= r s))
		(CH r x)
		(CH s x)
		(GED x y)
		(not (ZEXI y))
		(not (GED y x))
		(not (INCH r y))
		(not (INCH s y))
		(not (INCH r s))
	)
)
		

)