(cl-text http://colore.oor.net/inch/definitions/inch_ch.clif

(cl-imports http://colore.oor.net/inch/definitions/inch_ov.clif)

(cl-comment "I-D4: chunk (equidmensional part)")

(forall (x y)
	(iff
		(CH x y)
		(and
			(INCH x y)
			(forall (z)
				(if
					(OV x z)
					(OV y z)
				)
			)
		)
	)
)

)