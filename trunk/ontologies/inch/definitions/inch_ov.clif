(cl-text http://colore.oor.net/inch/definitions/inch_gd.clif

(cl-comment "I-D2: overlap (PO)")

(forall (x y)
	(iff
		(OV x y)
		(and
			(INCH x y)
			(INCH y x)
		)
	)
)

)