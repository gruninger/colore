(cl-text http://colore.oor.net/inch/definitions/inch_ged.clif

(cl-imports http://colore.oor.net/inch/definitions/inch_zex.clif)

(cl-comment "I-D7: greater or equal dimension")

(forall (x y)
	(iff
		(GED x y)
		(or
			(ZEXI y)
			(exists (z)
				(and
					(INCH x z)
					(INCH z y)
				)
			)
		)
	)
)

)