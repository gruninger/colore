(cl-text multidim_mereotopology_codi/PLP-E2.clif

(cl-comment "PLP-E2: maximal three dimensions")

(forall (x y p)
	(if
		(and
			(Pl x)
			(Pl y)
			(not (= x y))
			(Pt p)
			(Cont p x)
			(Cont p y)
		)
		(exists (q)
			(and
				(Pt q)
				(not (= p q))
				(Cont q x)
				(Cont q y)
			)
		)
	)
)

)
