(cl-text multidim_mereotopology_codi/codi_pl_slin.clif

(cl-imports multidim_mereotopology_codi/codi_pl.clif)


(cl-comment "PL-A2: Line Axiom, Part I")

(forall (l m p q)
	(if
		(and
			(L l)
			(L m)
			(Pt p)
			(Pt q)
			(not (= p q))
			(Cont p l)
			(Cont q l)
			(Cont p m)
			(Cont q m)
		)
		(= l m)
	)
)		


)
