(cl-imports multidim_mereotopology_codi/theorems/codi_down_theoremsT11.clif)

(cl-comment "Dif-T11")

(forall (x y z)
	(if
		(and
			(P y x)
			(Min z)	
			(P z x)	
		)
		(or
			(P z (intersection x y))
			(P z (difference x y))
		)
	)
)


)