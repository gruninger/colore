(cl-text multidim_mereotopology_codi/theorems/codi_down_theoremsPO-E1.clif

(cl-imports multidim_mereotopology_codi/theorems/codi_down_theoremsEP-E3.clif)

(cl-imports multidim_mereotopology_codi/theorems/codi_down_theoremsT11.clif)


(cl-comment "parthood defined by PO")

(forall (x y)
	(if
		(and
			(not (ZEX x))
			(forall (z)
				(if
					(PO x z)
					(PO y z)
				)
			)
		)
		(P x y)
	)
)


(cl-comment "PO-E1: extensionality of PO")

(forall (x y)
	(if
		(forall (z)
			(iff
				(PO x z)
				(PO y z)
			)
		)
		(= x y)
	)
)

)