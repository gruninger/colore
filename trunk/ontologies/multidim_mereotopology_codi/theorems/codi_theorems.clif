(cl-text multidim_mereotopology_codi/codi_theorems.clif

(cl-imports multidim_mereotopology_codi/codi.clif)

(cl-imports multidim_mereotopology_codi/definitions/ep.clif)

(cl-imports multidim_mereotopology_codi/definitions/po.clif)

(cl-imports multidim_mereotopology_codi/definitions/inc.clif)

(cl-imports multidim_mereotopology_codi/definitions/sc.clif)


(cl-comment "CD-T1")

(forall (x y)
	(if
		(PO x y)
		(C x y)
	)
)

(cl-comment "CD-T2")

(forall (x y)
	(if
		(SC x y)
		(C x y)
	)
)

(cl-comment "CD-T3")

(forall (x y)
	(if
		(Inc x y)
		(C x y)
	)
)


(cl-comment "CD-T4")

(forall (x y)
	(if
		(C x y)
		(or
			(PO x y)
			(SC x y)
			(Inc x y)
		)
	)
)



(cl-comment "CD-T5")

(forall (x y)
	(if
		(PO x y)
		(not (SC x y))
	)
)


(cl-comment "CD-T6")

(forall (x y)
	(if
		(PO x y)
		(not (Inc x y))
	)
)


(cl-comment "CD-T7")

(forall (x y)
	(if
		(SC x y)
		(not (PO x y))
	)
)


(cl-comment "CD-T8")

(forall (x y)
	(if
		(SC x y)
		(not (Inc x y))
	)
)


(cl-comment "CD-T9")

(forall (x y)
	(if
		(Inc x y)
		(not (PO x y))
	)
)


(cl-comment "CD-T10")

(forall (x y)
	(if
		(Inc x y)
		(not (SC x y))
	)
)




)