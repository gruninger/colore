<?xml version="1.0" encoding="UTF-8"?>
<?xml-stylesheet type="text/xsl" href="../COLORE.xsl"?>

<cl-module 	xmlns:cl="http://stl.mie.utoronto.ca/ColoreSchema" 
					xmlns:lat="http://stl.mie.utoronto.ca/colore/foundational/lattices" 
					xmlns:pos="http://stl.mie.utoronto.ca/colore/foundational/posets" 
					xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
					xmlns="http://stl.mie.utoronto.ca/ColoreSchema"
					targetNamespace="http://stl.mie.utoronto.ca/colore/foundational/lattices">

	<moduleDefinition module="Latticeposet" axiomFile="lattice_poset.clif" />

	<head>
		<version>1.0</version>
		<date>2009-02-26</date>
		<description>Axioms for arbitrary lattices based on the axioms for posets</description>
		<keyword>Lattice</keyword>
		<keyword>poset</keyword>
		<keyword>Partial Order</keyword>
		<author>Torsten Hahmann</author>
		<parentOntology>
			<name>Lattices</name>
		</parentOntology>
		<source>
			<name>Axioms for Lattices and Boolean Algebras</name>
			<author>Padmanabhan</author>
			<author>Rudeanu</author>
			<year>2008</year>
		</source>
	</head>

	<lexicon/>

	<inheritedLexicon>
		<primitiveLexicalSymbol symbol="leq" type="relation" arity="2"/>
	</inheritedLexicon>

	<directRelations>
		<extends extensionType="non-conservative">pos:poset</extends>
		<definablyEquivalent mappingExtension="lat:lattice">lat:lattice_meet_join</definablyEquivalent>
	</directRelations>

	<inheritedRelations/>

	<additionalInformation/>

</cl-module>