
(cl-text http://colore.oor.net/planar_mereology/atomic_noncollinear_coplanar.clif

(cl-imports http://colore.oor.net/planar_mereology/planar_mereology_root.clif)

(forall (q l p1)
	(if	(and	(plane q)
			(in l q)
			(line l)
			(in p1 q)
			(point p1)
			(atomic_part p1))
		(exists (p2)
			(and	(point p2)
				(atomic_part p2)
				(in p2 q)
				(not (in p2 l))))))

)
