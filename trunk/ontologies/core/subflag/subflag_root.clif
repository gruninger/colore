
(cl-text subflag_root

(cl-imports weak_tripartite)

(forall (x y z)
	(if	(subflag x y z)
		(flag x y z)))

(forall (x y z)
	(if	(subflag x y z)
		(subflag z y x)))

(forall (x y z)
	(if	(subflag x y z)
		(subflag y z x)))

(forall (x y z)
	(if	(subflag x y z)
		(subflag x y y)))

(forall (x y z)
	(if	(subflag x y z)
		(subflag x x x)))

(forall (x y z)
	(iff	(flag x y z)
		(and	(in x y)
			(in y z)
			(in x z))))

)
