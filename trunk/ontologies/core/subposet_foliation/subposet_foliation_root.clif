
(cl-text subposet_foliation_root

(cl-imports ../mereological_foliation/m_foliation_root)

(forall (x y p)
	(if	(tleq x y p)
		(and	(line x)
			(line y)
			(point p))))

(forall (x y p)
	(if	(tleq x y p)
		(and	(in x p)
			(in y p))))

(forall (x p)
	(if	(line x)
		(tleq x x p)))

(forall (x y p)
        (if     (and	(tleq x y p)
			(tleq y x p))
		(= x y)))

(forall (x y z p)
        (if     (and    (tleq x y p)
                        (tleq y z p))
                (tleq x z p)))

)
