
(cl-text m_bundle

(cl-imports ../bipartite_incidence/weak_bipartite)

(forall (x y l)
	(if	(tpart x y l)
		(and	(point x)
			(point y)
			(line l))))

(forall (x l)
	(if	(point x)
		(tpart x x l)))

(forall (x y l)
        (if     (and	(tpart x y l)
			(tpart y x l))
		(= x y)))

(forall (x y z l)
        (if     (and    (tpart x y l)
                        (tpart y z l))
                (tpart x z l)))

)
