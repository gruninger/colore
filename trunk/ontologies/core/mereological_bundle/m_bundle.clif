
(cl-text m_bundle

(cl-imports ../bipartite_incidence/weak_bipartite)

(forall (x y p)
	(if	(tpart x y p)
		(and	(line x)
			(line y)
			(point p))))

(forall (x p)
	(if	(line x)
		(tpart x x p)))

(forall (x y p)
        (if     (and	(tpart x y p)
			(tpart y x p))
		(= x y)))

(forall (x y z p)
        (if     (and    (tpart x y p)
                        (tpart y z p))
                (tpart x z p)))

)
