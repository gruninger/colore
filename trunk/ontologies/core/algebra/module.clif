(cl-text module

(cl-imports ../ringoid/ring)

(cl-imports .../magma/abelian_group)

(forall (r x y)
	(= (mult r (op x y)) (op (mult r x) (mult r y))))

(forall (r s x)
	(= (mult (sum r s) x) (op (mult r x) (mult s x))))

(forall (r s x)
	(= (mult (prod r s) x) (mult r (mult s x))))

(forall (x)
	(= x (mult one x)))

)
