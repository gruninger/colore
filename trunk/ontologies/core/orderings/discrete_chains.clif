
(cl-text discrete_chains

(cl-imports chains)

(forall (x y)
	(if	(lt x y)
		(exists (z)
			(and	(covers y z)
				(leq x z)))))

(forall (x y)
	(if	(lt x y)
		(exists (z)
			(and	(covers z x)
				(leq z y)))))

(forall (x y)
	(iff	(covers y x)
		(and	(lt x y)
			(not (exists (z)
				(and	(lt x z)
					(lt z y)))))))
)
