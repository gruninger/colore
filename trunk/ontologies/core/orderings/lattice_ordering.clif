
(cl-text lattice_ordering

(cl-imports partial_ordering)

(cl-comment "Unique meet")

(forall (x y) 
	(exists (m)
		(and 	(leq m x)
			(leq m y)
			(forall (z)
				(if	(and	(leq z x)
						(leq z y))
					(leq z m))))))
						
(cl-comment "Unique join")

(forall (x y) 
	(exists (j)
		(and 	(leq x j)
			(leq y j)
			(forall (z)
				(if	(and	(leq x z)
						(leq y z))
					(leq j z))))))

)
