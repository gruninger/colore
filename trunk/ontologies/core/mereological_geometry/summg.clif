
(cl-text summg

(cl-imports wmg)

(forall (x y l)
        (if     (and	(underlaps_line x y)
			(line l)
			(in x l)
			(in y l))
                (exists (z)
			(and	(point z)
				(in z l)
                        	(forall (v)
					(if	(and	(point v)
							(in v l))
                                		(iff    (overlaps_line v z)
                                        		(or     (overlaps_line v x)
                                                		(overlaps_line v y)))))))))


)
