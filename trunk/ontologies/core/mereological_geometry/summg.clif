
(cl-text summg

(cl-imports wmg)

(cl-imports mg_def)

(forall (x y l)
        (if     (underlaps_line x y l)
                (exists (z)
			(and	(point z)
				(in z l)
                        	(forall (v)
					(if	(and	(point v)
							(in v l))
                                		(iff    (overlaps_line v z l)
                                        		(or     (overlaps_line v x l)
                                                		(overlaps_line v y l)))))))))


)
