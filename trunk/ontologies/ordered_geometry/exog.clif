
(cl-text http://colore.oor.net/ordered_geometry/exog

(cl-imports http://colore.oor.net/ordered_geometry/wog)

(forall (l)
	(if	(line l)
		(exists (x y)
			(and	(point x)
				(point y)
				(in x l)
				(in y l)
				(forall (z)
					(if	(and	(point z)
							(in z l))
						(between x z y)))))))

)
