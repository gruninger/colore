(cl-text http://colore.oor.net/ordered_geometry/wbog.clif

(cl-imports http://colore.oor.net/ordered_geometry/wog.clif)

(forall (x y l)
	(if	(and	(B x y x)
			(line l)
			(in x l)
			(in y l))
		(= x y)))

(forall (x y z l)
	(if	(and	(B x y z)
			(B y x z)
			(line l)
			(in x l)
			(in y l)
			(in z l))
		(= x y)))

)
