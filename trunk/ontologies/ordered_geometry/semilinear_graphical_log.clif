
(cl-text semilinear_graphical_log

(cl-imports graphical_incidence)

(cl-imports log)

(forall (x y z)
	(if	(and	(point x)
			(point y)
			(point z))
		(iff	(and	(between y x z)
				(between x y z))
			(= x y))))

(forall (x y z w)
	(if	(and	(point x)
			(point y)
			(point z)
			(between y x z))
		(or	(between y x w)
			(between z x w))))

)
