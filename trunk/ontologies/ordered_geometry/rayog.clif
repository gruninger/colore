
(cl-text http://colore.oor.net/ordered_geometry/rayog.clif

(cl-imports http://colore.oor.net/ordered_geometry/wog.clif)

(forall (l)
	(if	(line l)
		(exists (x)
			(and	(point x)
				(in x l)
				(forall (y)
					(if	(and	(point y)
							(in y l))
						(exists (z)
							(and	(point z)
								(in z l))
								(between x y z)))))))))

)
