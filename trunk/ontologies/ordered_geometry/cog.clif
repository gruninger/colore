
(cl-text http://colore.oor.net/ordered_geometry/cog.clif

(cl-imports http://colore.oor.net/ordered_geometry/wog.clif)

(forall (x y z w u l)
	(if	(and	(between x y z)
			(between x w z)
			(between y u w)
			(in x l)
			(in z l)
			(point x)
			(point y)
			(point z)
			(point w)
			(point u)
			(line l))
		(and	(in u l)
			(between x u z))))

)
