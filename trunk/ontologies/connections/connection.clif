
(cl-text http://colore.oor.net/connections/connection.clif


(cl-comment "C1: Reflexivity")

(forall (x) 
	(if 	(not (= x 0))
		(c x x)))

(cl-comment "C2: Symmetry")

(forall (x y)
	(if 	(c x y)
		(c y x)))
		
(cl-comment "C-Ext")

(forall (x y)
	(if
		(forall (z)
			(iff	(C x z)
				(C y z)))
		(= x y)))

)

