
(cl-text owltime_inside2ordered_geometry

(forall (x y)
	(iff	(in x y)
		(or	(and	(Instant x)
				(Interval y)
				(inside x y))
			(and	(Interval x)
				(Instant y)
				(inside y x)))))

(forall (x y z)
	(iff	(between x y z)
		(and	(Instant x)
			(Instant y)
			(Instant z)
			(or	(and	(before x y)
					(before y z))
				(and	(before z y)
					(before y z))))))

)
