
(cl-text owltime2bipartite_incidence

(forall (x)
	(iff	(point x)
		(Instant x)))

(forall (x)
	(iff	(line x)
		(Interval x)))

(forall (x y)
	(iff	(in x y)
		(or	(and	(Instant x)
				(or	(begins x y)
					(ends x y)))
			(and	(Interval y)
				(= x y))
			(and	(Interval x)
				(Instant y)
				(or	(begins y x)
					(ends y x))))))

)
