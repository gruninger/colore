
(cl-text http://colore.oor.net/owltime_instants/owltime_instant_s.clif

(cl-imports http://colore.oor.net/owltime_instants/owltime_instants.clif)

(forall (t1 t2)
	(if	(and	(Instant t1)
			(Instant t2))
		(exists (z)
			(and	(Instant z)
				(or	(before z t1)
					(= z t1))
				(or	(before z t2)
					(= z t2))))))
			

)
