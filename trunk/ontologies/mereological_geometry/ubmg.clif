
(cl-text http://colore.oor.net/mereological_geometry/ubmg.clif

(cl-imports http://colore.oor.net/mereological_geometry/wmg.clif)

(forall (x y l)
	(if	(and	(point x)
			(point y)
			(line l)
			(in x l)
			(in y l))
		(exists (z)
			(and	(point z)
				(in z l)
				(part x z)
				(part y z)))))

)
