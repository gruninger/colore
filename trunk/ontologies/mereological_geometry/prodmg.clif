
(cl-text prodmg

(cl-imports wmg)

(cl-imports mg_def)

(forall (x y l)
        (if     (overlaps_line x y l)
                (exists (z)
			(and	(point z)
				(in z l)
                        	(forall (v)
					(if	(and	(point v)
							(in v l))
                                		(iff    (ppart v z)
                                        		(and    (ppart v x)
                                                		(ppart v y)))))))))


)
