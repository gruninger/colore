(cl-module point (cl-imports http://colore.oor.net/mereology/mereology_def.clif))

(forall (x y l)
	(if	(and	(point x)
			(point y)
			(line l)
			(in x l)
			(in y l)
			(prod x y z))
		(in z l)))

)
