
(cl-text mem_mereology

(cl-comment "Minimal Extensional Mereology Simons 1987")

(cl-imports mm_mereology)

(cl-imports mereology_def)

(cl-comment "Product")
(forall (x y)
        (if     (overlaps x y)
                (exists (z)
                        (forall (v)
                                (iff    (ppart v z)
                                        (and    (ppart v x)
                                                (ppart v y)))))))

)
