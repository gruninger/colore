
(cl-text http://colore.oor.net/mereology/em_mereology.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(cl-imports http://colore.oor.net/mereology/mereology_def.clif)

(cl-comment "Strong Supplementation Principle")
(forall (x y)
        (if     (not (part x y))
                (exists (z)
                        (and    (part z x)
                                (disjoint z y)))))

)
