
(cl-text http://colore.oor.net/mereology/exm_mereology.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(cl-imports http://colore.oor.net/mereology/mereology_def.clif)

(cl-comment "Extensionality")
(forall (x y w)
        (if     (and	(ppart w x)
			(not (= x y)))
                (exists (z)
                        (iff    (ppart z x)
                                (not (ppart z y))))))

)
