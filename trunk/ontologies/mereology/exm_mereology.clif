
(cl-text exm_mereology

(cl-imports m_mereology)

(cl-imports mereology_def)

(cl-comment "Extensionality")
(forall (x y w)
        (if     (and	(ppart w x)
			(not (= x y)))
                (exists (z)
                        (iff    (ppart z x)
                                (not (ppart z y))))))

)
