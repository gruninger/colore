
(cl-text http://colore.oor.net/mereology/mm_mereology.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(cl-imports http://colore.oor.net/mereology/mereology_def.clif)

(cl-comment "Weak Supplementation Principle")
(forall (x y)
        (if     (ppart x y)
                (exists  (z)
                        (and    (ppart z y)
                                (disjoint z x)))))


)
