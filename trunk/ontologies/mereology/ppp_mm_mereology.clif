
(cl-text http://colore.oor.net/mereology/ppp_mm_mereology.clif

(cl-imports http://colore.oor.net/mereology/mm_mereology.clif)

(cl-imports http://colore.oor.net/mereology/mereology_def.clif)

(cl-comment "Proper Parts Principle")
(forall (x y w)
        (if     (and	(ppart w x)
		 	(not (part x y)))
		(exists (z)
			(and	(ppart z x)
				(not (ppart z y))))))

)
