
(cl-text http://colore.oor.net/mereology/tree-mereology.clif

(cl-imports http://colore.oor.net/mereology/discrete_mereology.clif)

(cl-imports http://colore.oor.net/mereology/mereology_def.clif)

(forall (x y)
	(if	(overlaps x y)
		(or	(part x y)
			(ppart y x))))

(forall (x y)
	(if	(ppart x y)
		(exists (z)
			(and	(ppart z y)
				(not (overlaps x z))))))

)
