
(cl-text inclusion_space

(cl-imports dense_mereology)

(cl-imports mereology_def)

(forall (x y)
	(exists (z)
		(and	(part x z)
			(part y z))))

(forall (x)
	(exists (y z)
		(and	(ppart y x)
			(ppart x z))))

(forall (x y)
	(if	(not (part x y))
		(exists (z)
			(and	(ppart z x)
				(not (ppart z y))))))

)
