
(cl-text http://colore.oor.net/mereology/inclusion_space.clif

(cl-imports http://colore.oor.net/mereology/dense_mereology.clif)

(cl-imports http://colore.oor.net/mereology/mereology_def.clif)

(forall (x y)
	(exists (z)
		(and	(part x z)
			(part y z))))

(forall (x)
	(exists (y z)
		(and	(ppart y x)
			(ppart x z))))

(forall (x y)
	(if	(not (part x y))
		(exists (z)
			(and	(ppart z x)
				(not (ppart z y))))))

)
