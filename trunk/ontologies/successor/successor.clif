
(cl-text http://colore.oor.net/successor/successor.clif

(cl-imports http://colore.oor.net/successor/definitions/successor_def.clif)

(forall (x y z)
	(if	(and	(S x y)
			(S x z))
		(= y z)))

(forall (x y z)
	(if	(and	(S x y)
			(S z y))
		(= x z)))

(forall (y)
	(if	(not (zero y))
		(exists (x)
			(S x y))))

)
