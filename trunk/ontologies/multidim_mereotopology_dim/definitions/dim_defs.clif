(cl-text multidim_mereotopology_dim/definitions/dim_defs.clif

(cl-comment "definitional extensions to basic axioms of dimensionality")

(cl-imports multidim_mereotopology_dim/dim_basic.clif)
(cl-imports multidim_mereotopology_dim/definitions/min_max_dim.clif)
(cl-imports multidim_mereotopology_dim/definitions/dim_covers.clif)


(cl-comment "D-D1: >")

(forall (x y)
	(iff (> x y) (< y x))
)


(cl-comment "D-D2: >=")

(forall (x y)
	(iff (>= x y) (<= y x))
)


(cl-comment "D-D3: <=")

(forall (x y)
	(iff (<= x y) (or (< x y) (EqDim x y)))
)


(cl-comment "D-D6: EqDimPossible")

(forall (x y)
	(iff 	(EqDimPossible x y)
		(and 	
			(not (< x y))
			(not (< y x))
		)
	)
)


)



