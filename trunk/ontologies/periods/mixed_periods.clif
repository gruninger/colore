(cl-text mixed_periods

(cl-imports ordered_periods)

(cl-imports finite_periods)

(cl-imports periods_def)

(cl-comment "NEIGH-1")
(forall (x y)
(if		(precedence x y)
		(exists (w)
				(and	(precedence x w)
						(not	(exists (z)
										(and	(precedence x z)
												(precedence z w))))))))

(cl-comment "NEIGH-2")
(forall (x y)
	(if	(precedence y x)
		(exists (w)
			(and	(precedence w x)
				(not (exists (z)
					(and	(precedence w z)
						(precedence z x))))))))

)
