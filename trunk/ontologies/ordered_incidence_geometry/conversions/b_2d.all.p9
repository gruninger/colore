% 'B1: outer symmetry'
 all a all b all c ((B(a, b, c) -> B(c, b, a))).
% 'B2: strict, i.e. acyclic'
 all a all b all c ((B(a, b, c) -> -(B(b, c, a)))).
% 'B3: linear coherence'
