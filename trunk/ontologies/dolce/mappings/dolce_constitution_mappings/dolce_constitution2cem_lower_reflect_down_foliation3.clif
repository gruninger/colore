	(iff (part3 x y)
		(and (P x y)(T x)(T y))))

(forall (x y)
	(iff (in3 x y)
		(or (PRE x y)(PRE y x)(= x y))))

(forall (x)
	(iff (point3 x)(T x)))

(forall (x)
	(iff (line3 x)(PD x)))

(forall (x y z)
	(iff (tpart3 x y z)
		(and (tP x y z)(PD x)(PD y)(T z))))

(forall (x y z)
	(iff (tlt3 x y z)
		(and (K x y z)(PD x)(PD y)(T z))))

