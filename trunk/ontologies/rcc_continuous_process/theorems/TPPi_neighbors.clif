(cl-text http://colore.oor.net/rcc_continuous_process/theorems/TPPi_neighbors.clif

(cl-imports http://colore.oor.net/rcc_continuous_process/rcc_continuous_process.clif)


(forall (o x y)  
		(if 	(changes o (TPPs y x)) 
				(or		(changes o (POs x y))
						(changes o (EQs x y))
						(changes o (NTPPs y x)))))
	 
)
