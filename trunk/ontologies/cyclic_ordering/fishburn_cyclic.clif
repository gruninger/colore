
(cl-text http://colore.oor.net/cyclic_ordering/fishburn_cyclic

(forall (x y z)
	(if	(C x y z)
		(C y z x)))

(forall (x y z)
	(if	(C x y z)
		(not (= x y))))

(forall (x y z w)
	(if	(and	(C x y z)
			(C x z w))
		(C x y w)))

(forall (x y z a b d)
	(if	(and	(C x y z)
			(C x b d)
			(C a y d)
			(C a b z))
		(C a b d)))

)
