
(cl-text combined_time2periods

(forall (i1 i2)
	(iff	(precedence i1 i2)
		(and	(before (beginof i1) (endof i1))
			(before (beginof i2) (endof i2))
			(before (endof i1) (beginof i2)))))

(forall (i1 i2)
	(iff	(inclusion i1 i2)
		(and	(before (beginof i2) (beginof i1))
			(before (endof i1) (endof i2)))))

)
