
(cl-text no_backwards

(cl-imports sim_vc_end)

(forall (i1)
	(if	(timeinterval i1)
		(not (exists (i2)
			(and	(timeinterval i2)
				(= (beginof i2) (endof i1))
				(= (endof i2) (beginof i1))
                                (= (beginof i1) (endof i1))
                                (= (beginof i2) (endof i2)))))))


)
