
(cl-text hayes_endpoints

(cl-imports linear_point)

(forall (i)
	(if	(timeinterval i) 
		(before (beginof i) (endof i))))

(forall (i)
	(if	(timeinterval i) 
		(= i (between (beginof i) (endof i)))))

(forall (p q)
	(iff	(before p q) 
		(and	(= p (beginof (between p q))) 
			(= q (endof (between p q)))))) 


)
