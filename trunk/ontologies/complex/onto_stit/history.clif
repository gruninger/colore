(cl-text history

(cl-imports moment)

(forall (x y)
(if	(in x y)
	(and	(mo x)
		(ht y))))

(forall (x)
(if	(mo x)
	(not (ht y))))

(forall (x)
(if	(mo x)
	(exists (y)
		(in x y))))

(forall (x y z)
(if	(and	(ht z)
		(in x z)
		(in y z))
	(or	(= x y)
		(pre x y)
		(pre y x))))

(forall (x y)
(if	(and	(ht x)
		(ht y)
		(not (= x y)))
	(exists (z)
		(and	(in z x)
			(not (in z y))))))

)
