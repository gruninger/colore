
(cl-text http://colore.oor.net/ontologies/ordered_arithmetic/oa

(cl-imports http://colore.oor.net/ontologies/ordering/discrete_forest)

(cl-imports http://colore.oor.net/ontologies/cayley_graph/cayley_graph_root)

(forall (x y z)
	(if	(generator y)
		(iff	(lt x (append y z))
			(leq x z))))

(forall (x y)
	(if	(lt x y)
		(exists (z w)
			(= y (append z w)))))

)
