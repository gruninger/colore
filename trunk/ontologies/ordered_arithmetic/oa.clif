
(cl-text http://colore.oor.net/ontologies/ordered_arithmetic/oa.clif

(cl-imports http://colore.oor.net/ontologies/ordering/discrete_forest.clif)

(cl-imports http://colore.oor.net/ontologies/cayley_graph/cayley_graph_root.clif)

(forall (x y z)
	(if	(generator y)
		(iff	(lt x (append y z))
			(leq x z))))

(forall (x y)
	(if	(lt x y)
		(exists (z w)
			(= y (append z w)))))

)
