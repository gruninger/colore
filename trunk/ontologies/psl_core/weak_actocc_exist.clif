(cl-text http://colore.oor.net/psl_core/weak_actocc_exist.clif

(cl-imports http://colore.oor.net/psl_core/psl_core_root.clif)

(forall (o t)
	(if	(and	(activity_occurrence o)
			(is_occurring_at o t))
		(exists (x)
			(participates_in x o t))))

)
