(cl-text multidim_mereotopology_codib/codib_tcont_theorems.clif

(cl-imports multidim_mereotopology_codib/codib.clif)

(cl-imports multidim_mereotopology_codib/definitions/tcont.clif)

(cl-imports multidim_mereotopology_codib/definitions/closed.clif)


(cl-comment 'TC-T1')

(forall (x y)
	(if
		(TCont x y)
		(Cont x y)
	)
)

(cl-comment 'TC-T2')

(forall (x y)
	(if
		(BCont x y)
		(TCont x y)
	)
)


(cl-comment 'TC-T3')

(forall (x)
	(if
		(Closed x)
		(forall (y)
			(not (TCont y x))
		)
	)
)


(cl-comment 'TC-T4')

(forall (x)
	(if
		(Closed x)
		(not (TCont x x))
	)
)

(cl-comment 'TC-T5')

(forall (x)
	(if
		(not (Closed x))
		(TCont x x)
	)
)


)