(cl-text multidim_mereotopology_codib/codi_bcont_U-A1_theorems.clif

(cl-imports multidim_mereotopology_codib/codi_bcont_U-A1.clif)


(cl-comment 'BC-T4')

(forall (x y z)
	(if
		(and
			(SC x y)
			(Min x)
			(MaxDim x)
			(Cont z x)
			(Cont z y)
		)
		(BCont z x)
	)
) 


)