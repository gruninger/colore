(cl-text multidim_mereotopology_codib/codib_icont_theorems.clif

(cl-imports multidim_mereotopology_codib/codib.clif)

(cl-imports multidim_mereotopology_codib/definitions/icont.clif)

(cl-imports multidim_mereotopology_codib/definitions/closed.clif)


(cl-comment 'IC-T1')

(forall (x y)
	(if
		(ICont x y)
		(Cont x y)
	)
)

(cl-comment 'IC-T2')

(forall (x y)
	(or
		(not (ICont x y))
		(not (BCont x y))
	)
)

(cl-comment 'IC-T3')

(forall (x y)
	(if
		(and
			(Closed x)
			(Cont y x)
		)
		(ICont y x)
	)
)

(cl-comment 'IC-T4')

(forall (x)
	(if
		(and
			(Closed x)
			(not (ZEX x))
		)
		(ICont x x)
	)
)

(cl-comment 'IC-T5')

(forall (x)
	(if
		(not (Closed x))
		(not (ICont x x))
	)
)

(cl-comment 'IC-T6')

(forall (x y z)
	(if
		(and
			(Cont x y)
			(ICont y z)
		)
		(ICont x z)
	)
)

)