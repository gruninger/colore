(cl-text multidim_mereotopology_codib/codib_updown_theorems.clif

(cl-imports multidim_mereotopology_codib/codib_updown.clif)

(cl-imports multidim_mereotopology_codi/definitions/sc.clif)

(cl-comment 'Sum-T1prime') 

(forall (x y z v)
	(if
		(and
			(TSum x y v)
			(EqDim x y)
			(SC x y)
			(Cont z x)
			(Cont z y)
		)
		(BCont z x)
	)
)

)