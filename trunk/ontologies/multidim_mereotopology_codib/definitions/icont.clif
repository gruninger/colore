(cl-text multidim_mereotopology_codib/definitions/icont.clif

(cl-imports multidim_mereotopology_codib/codib.clif)

(cl-comment 'Defining interior containment')

(cl-comment 'IC-D')

(forall (x y)
	(iff
		(ICont x y)
		(and
			(Cont x y)
			(forall (z)
				(if
					(Cont z x)
					(not (BCont z y))
				)
			)
		)
	)
)


)