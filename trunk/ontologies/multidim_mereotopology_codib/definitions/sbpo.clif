(cl-text multidim_mereotopology_codib/definitions/sbpo.clif

(cl-imports multidim_mereotopology_codib/codib.clif)

(cl-imports multidim_mereotopology_codib/definitions/sbp.clif)


(cl-comment 'SBPO-D: strong boundary part overlap')

(forall (x y)
	(iff
		(SBPO x y)
		(and
			(PO x y)
			(SBP (intersection x y) x)
			(SBP (intersection x y) y)
		)
	)
)



)