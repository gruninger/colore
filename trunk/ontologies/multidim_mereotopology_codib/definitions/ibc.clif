(cl-text multidim_mereotopology_codib/definitions/ibc.clif

(cl-imports multidim_mereotopology_codib/codib_down.clif)

(cl-comment 'Defining interior-boundary overlap')

(cl-comment 'IBC-D')

(forall (x y)
	(iff
		(IBC x y)
		(exists (z)
			(and
				(Cont z x)
				(not (BCont z x))
				(BCont z y)
			)
		)
	)
)


)