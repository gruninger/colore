(cl-text multidim_mereotopology_codib/consistency/codib_updown_nontrivial_simple.clif

(cl-imports multidim_mereotopology_codib/codib_updown.clif)

(cl-imports multidim_mereotopology_codi/definitions/epp.clif)

(cl-imports multidim_mereotopology_codi/definitions/connected.clif)

(exists (x y z r s t v)
	(and
		(BCont x y)
		(Cont x z)
		(not (BCont x z))
		(SC y z)
		(not (Con r))
		(PP t r)
		(PP s r)
		(= (difference r t) s)
		(BCont v t)
		(BCont v x)
		(Inc t x)
		(< t x) 
	)
)


)
