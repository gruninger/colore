
(cl-text owltime_inside

(cl-imports owltime_interval)

(forall (t x)
	(if	(inside t x)
		(and	(Instant t)
			(Interval x))))

(forall (t x)
	(iff	(beginsOrIn t x)
		(or	(begins t x)
			(inside t x))))

(forall (t t1 t2 x)
	(if	(and	(inside t x)
			(begins t1 x)
			(ends t2 x)
			(ProperInterval x))
		(and	(before t1 t)
			(before t t2))))


)
