
(cl-text http://colore.oor.net/quadripartite_incidence/diamond_quadripartite.clif

(cl-imports http://colore.oor.net/quadripartite_incidence/near4diamond.clif)

(forall (p q)
	(if	(and	(space q)
			(line p)
			(in p q))
		(exists (l1 l2)
			(and	(plane l1)
				(plane l2)
				(in p l1)
				(in p l2)
				(in l1 q)))))

)
