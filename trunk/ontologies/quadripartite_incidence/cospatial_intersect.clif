
(cl-text http://colore.oor.net/quadripartite_incidence/cospatial_intersect.clif

(cl-imports http://colore.oor.net/quadripartite_incidence/weak_quadripartite.clif)

(forall (p l1 l2 s)
	(if	(and	(point p)
			(line l1)
			(line l2)
			(plane s)
			(in p l1)
			(in p l2)
			(in l1 s))
		(in l2 s)))
)
