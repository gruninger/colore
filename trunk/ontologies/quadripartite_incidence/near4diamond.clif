
(cl-text near4diamond

(cl-imports quadripartite)

(cl-imports diamond_tripartite)

(forall (p q l1 l2 l3)
	(if	(and	(space q)
			(line p)
			(in p q)
			(plane l1)
			(plane l2)
			(plane l3)
			(in p l1)
			(in p l2)
			(in p l3)
			(in l1 q)
			(in l2 q)
			(in l3 q))
		(or	(= l1 l2)
			(= l1 l3)
			(= l2 l3))))	

)
