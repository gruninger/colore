(cl-text http://colore.oor.net/magma/unipotent_magma.clif

(cl-imports http://colore.oor.net/magma/magma.clif)

(cl-imports http://colore.oor.net/magma/definitions/magma_defs.clif)

(forall (x y)
	(if	(inverse x y)
		(and	(identity (op x y))
			(identity (op y x)))))

(forall (x y)
	(if	(inverse x y)
		(= (op x y) (op y x))))

)
