(cl-text http://colore.oor.net/magma/e_semigroup.clif

(cl-imports http://colore.oor.net/magma/semigroup.clif)

(cl-imports http://colore.oor.net/magma/definitions/magma_defs.clif)

(forall (x y z)
	(iff	(weak_inverse (op x y) z)
		(exists (u v)
			(and	(weak_inverse x u)
				(weak_inverse y v)
				(= (op u v) z)))))
		

)
