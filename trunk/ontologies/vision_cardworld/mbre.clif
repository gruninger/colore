(cl-text http://colore.oor.net/vision_cardworld/mbre.clif

(cl-imports http://colore.oor.net/vision_cardworld/kernel.clif)

(forall (l r e s)
	(if	(and	(part e s)
			(depict l e)
			(in l r)
			(not (depicts r s)))
		(exists (r1)
			(and	(in l r1)
				(not (= r r1))))))

)
