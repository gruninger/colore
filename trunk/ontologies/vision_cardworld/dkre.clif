(cl-text http://colore.oor.net/vision_cardworld/dkre.clif

(cl-imports http://colore.oor.net/vision_cardworld/kernel.clif)

(forall (l e s)
	(if	(and	(part e s)
			(depict l e))
		(exists (r)
			(depict r s))))

)
