
(cl-text http://colore.oor.net/betweenness_bundle/b_bundle.clif

(cl-imports http://colore.oor.net/torsten-phd/btw/btw_root.clif)

(cl-imports http://colore.oor.net/bipartite_incidence/weak_bipartite.clif)

(forall (l x y z)
	(if	(Btw l x y z)
		(and	(line l)
			(point x)
			(point y)
			(point z))))

(forall (l x y z)
	(if	(Btw l x y z)
		(and	(in x l)
			(in y l)
			(in z l))))

)
