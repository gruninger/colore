(cl-text http://colore.oor.net/multidim_space_physcont/definitions/mfinside.clif

(cl-comment 'detachable containment from 2013 COSIT paper')

(cl-imports http://colore.oor.net/multidim_space_physcont/definitions/minside.clif)

(cl-comment 'MFInside-D')

(forall (x y)
	(iff
		(mat-fills-inside x y)
		(and
			(mat-inside x y)
			(= (r x) (r y))
		)
	)
)


)