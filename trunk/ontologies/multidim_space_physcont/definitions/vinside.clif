(cl-text http://colore.oor.net/multidim_space_physcont/definitions/vinside.clif

(cl-comment 'detachable containment from 2013 COSIT paper')

(cl-imports http://colore.oor.net/multidim_space_physcont/definitions/inside.clif)

(cl-comment 'VInside-D')

(forall (x y)
	(iff
		(void-inside x y)
		(and
			(inside x y)
			(V x)
		)
	)
)


)