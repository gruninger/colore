(cl-text http://colore.oor.net/multidim_space_codib/definitions/tcont.clif

(cl-comment "Defining tangential containment")

(cl-comment "TC-D")

(forall (x y)
	(iff
		(TCont x y)
		(and
			(Cont x y)
			(exists (z)
				(and
					(Cont z x)
					(BCont z y)
				)
			)
		)
	)
)


)