
(cl-text http://colore.oor.net/tripartite_incidence/isolated_trivial_line.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (l q)
	(if	(and	(plane q)
			(line l)
			(in l q))
		(exists (p1 p2)
			(and	(point p1)
				(point p2)
				(not (= p1 p2))
				(in p1 l)
				(in p2 l)))))

)
