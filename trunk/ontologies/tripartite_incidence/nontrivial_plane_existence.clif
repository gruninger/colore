
(cl-text http://colore.oor.net/tripartite_incidence/nontrivial_plane_existence.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (l p1 p2)
	(if	(and	(line l)
			(point p1)
			(point p2)
			(not (= p1 p2))
			(in p1 l)
			(in p2 l))
		(exists (q)
			(and	(plane q)
				(in l q)))))

)
