
(cl-text http://colore.oor.net/tripartite_incidence/planar_double_points.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (x y z q)
	(if	(and	(point x)
			(point y)
			(point z)
			(plane q)
			(in x q)
			(in y q)
			(in z q))
		(or	(= z x)
			(= z y)
			(= x y))))

)
