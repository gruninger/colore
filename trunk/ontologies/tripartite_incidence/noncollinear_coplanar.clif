
(cl-text http://colore.oor.net/tripartite_incidence/noncollinear_coplanar

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite)

(forall (q l p1)
	(if	(and	(plane q)
			(line l)
			(in l q)
			(point p1)
			(in p1 q))
		(exists (p2)
			(and	(point p2)
				(in p2 q)
				(not (in p2 l))))))

)
