
(cl-text http://colore.oor.net/tripartite_incidence/diamond_tripartite.clif

(cl-imports http://colore.oor.net/tripartite_incidence/near3diamond.clif)

(forall (p q)
	(if	(and	(plane q)
			(point p)
			(in p q))
		(exists (l1 l2)
			(and	(line l1)
				(line l2)
				(not (= l1 l2))
				(in p l1)
				(in p l2)
				(in l1 q)
				(in l2 q)))))

)
