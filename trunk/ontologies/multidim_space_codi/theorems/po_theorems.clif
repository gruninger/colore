(cl-text http://colore.oor.net/multidim_space_codi/po_theorems.clif

(cl-imports http://colore.oor.net/multidim_space_codi/codi_basic.clif)

(cl-imports http://colore.oor.net/multidim_space_cont/definitions/c.clif)

(cl-imports http://colore.oor.net/multidim_space_codi/definitions/ep.clif)

(cl-imports http://colore.oor.net/multidim_space_codi/definitions/po.clif)

(cl-comment 'PO-T1: partial overlap reflexive')

(forall (x)
	(if
        (and
            (S x)
            (not (ZEX x))
        )
		(PO x x)
	)
)


(cl-comment 'PO-T2: partial overlap symmetric')

(forall (x y)
	(if
		(PO x y)
		(PO y x)
	)
)


(cl-comment 'PO-T3: partial overlap requires equi-dimensionality')

(forall (x y)
	(if
		(PO x y)
		(EqDim x y)
	)
)


)