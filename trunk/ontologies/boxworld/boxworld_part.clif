
(cl-text http://colore.oor.net/boxworld/boxworld_part.clif

(cl-imports http://colore.oor.net/boxworld/cardworld_part.clif)

(forall (x)
        (if     (or (point x) (edge x) (surface x))
                (not (box x))))

(forall (x y)
        (if     (and    (part x y) (box x) (box y))
                (= x y)))
)

