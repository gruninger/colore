
(cl-text http://colore.oor.net/subposet_bundle/subposet_bundle_root.clif

(cl-imports http://colore.oor.net/mereological_bundle/m_bundle.clif)

(forall (x y p)
	(if	(tlt x y p)
		(and	(line x)
			(line y)
			(point p))))

(forall (x y p)
	(if	(tlt x y p)
		(and	(in x p)
			(in y p))))

(forall (x y p)
	(if	(tlt x y p)
		(not (tlt y x p))))

(forall (x y z p)
        (if     (and    (tlt x y p)
                        (tlt y z p))
                (tlt x z p)))

)
