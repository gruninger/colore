
(cl-text http://colore.oor.net/biparite_incidence/double_points

(cl-imports http://colore.oor.net/biparite_incidence/weak_bipartite)

(cl-comment "A geometry in which at most two points can be incident with a line")

(forall (x y z l)
	(if	(and	(point x)
			(point y)
			(point z)
			(line l)
			(in x l)
			(in y l)
			(in z l))
		(or	(= z x)
			(= z y)
			(= x y))))

)
