
(cl-text http://colore.oor.net/biparite_incidence/double_exist.clif

(cl-imports http://colore.oor.net/biparite_incidence/weak_bipartite.clif)

(forall (x y)
	(if	(and	(point x)
			(point y)
			(not (= x y)))
		(exists (l1 l2)
			(and	(line l1)
				(line l2)
				(not (= l1 l2))
				(in x l1)
				(in y l1)
				(in x l2)
				(in y l2)))))

)
