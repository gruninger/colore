
(cl-text double_exist)

(cl-imports weak_bipartite)

(forall (x y)
	(if	(and	(point x)
			(point y)
			(not (= x y)))
		(exists (l1 l2)
			(and	(line l1)
				(line l2)
				(in x l1)
				(in y l1)
				(in x l2)
				(in y l2)))))

(forall (x y l1 l2 l3)
	(if	(and	(point x)
			(point y)
			(not (= x y))
			(line l1)
			(line l2)
			(line l3)
			(in x l1)
			(in x l2)
			(in x l3)
			(in y l1)
			(in y l2)
			(in y l3))
		(or	(= l1 l2)
			(= l1 l3)
			(= l2 l3))))

)
