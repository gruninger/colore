
(cl-text http://colore.oor.net/bipartite_incidence/parallel_trivial.clif

(cl-imports http://colore.oor.net/bipartite_incidence/weak_bipartite.clif)

(forall (x y p1)
	(if	(and	(line x)
			(line y)
			(not (= x y))
			(point p1)
			(in p1 x)
			(in p1 y))
		(exists (p2)
			(and	(point p2)
				(not (= p1 p2))
				(or	(in p2 x)
					(in p2 y))))))

)
