
(cl-text http://colore.oor.net/biparite_incidence/unique_parallel

(cl-imports http://colore.oor.net/biparite_incidence/weak_bipartite)

(forall (p1 l1 l2)
	(if	(and	(point p1)
			(line l1)
			(not (in p1 l1))
			(line l2)
			(in p1 l2)
			(parallel l1 l2)
			(line l3)
			(in p1 l3)
			(not (= l2 l3)))
		(exists (p2)
			(and	(point p2)
				(in p2 l1)
				(in p2 l3)))))

)
