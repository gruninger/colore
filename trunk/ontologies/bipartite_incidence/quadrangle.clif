
(cl-text http://colore.oor.net/biparite_incidence/quadrangle

(cl-imports http://colore.oor.net/biparite_incidence/polar_space)

(forall (p1 l1 p2 p3 l2 l3)
	(if	(and	(point p1)
			(not (in p1 l1))
			(point p2)
			(point p3)
			(in p2 l1)
			(in p3 l1)
			(in p1 l2)
			(in p2 l2)
			(in p1 l3)
			(in p2 l3))
		(or	(= p2 p3)
			(= l2 l3))))

)
