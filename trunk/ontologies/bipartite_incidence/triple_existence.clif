
(cl-text http://colore.oor.net/biparite_incidence/triple_existence.clif

(cl-imports http://colore.oor.net/biparite_incidence/weak_bipartite.clif)

(forall (x y w)
	(if	(and	(point x)
			(point y)
			(not (= x y))
			(point w)
			(not (= x w))
			(not (= y w)))
		(exists (z)
			(and	(line z)
				(in x z)
				(in y z)
				(in w z)))))

)
