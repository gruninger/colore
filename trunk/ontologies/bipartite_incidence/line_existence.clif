
(cl-text http://colore.oor.net/bipartite_incidence/line_existence.clif

(cl-imports http://colore.oor.net/bipartite_incidence/weak_bipartite.clif)

(cl-comment "There is a line through any two distinct points.")

(forall (x y)
	(if	(and	(point x)
			(point y)
			(not (= x y)))
		(exists (z)
			(and	(line z)
				(in x z)
				(in y z)))))

)
