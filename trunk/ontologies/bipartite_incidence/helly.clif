
(cl-text http://colore.oor.net/biparite_incidence/helly

(cl-imports http://colore.oor.net/biparite_incidence/weak_bipartite)

(forall (x y z)
	(if	(and	(point x)
			(point y)
			(point z))
		(exists (w)
			(and	(point w)
				(forall (l)
					(if	(and	(line l)
							(or	(in x l)
								(in y l))
							(or	(in x l)
								(in z l))
							(or	(in y l)
								(in z l)))
						(in w l)))))))

)
