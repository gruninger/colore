
(cl-text http://colore.oor.net/biparite_incidence/hilbert_parallel

(cl-imports http://colore.oor.net/biparite_incidence/hilbert_incidence)

(forall (p l1)
	(if	(and	(point p)
			(line l1)
			(not (in p l1)))
		(exists (l2)
			(and	(in p l2)
				(parallel l1 l2)))))

(forall (p l1 l2 l3)
	(if	(and	(in p l2)
			(parallel l1 l2)
			(in p l3)
			(parallel l1 l3))
		(= l2 l3)))

(forall (l1 l2)
	(iff	(parallel l1 l2)
		(not (exists (p)
			(and	(in p l1)
				(in p l2))))))

)
