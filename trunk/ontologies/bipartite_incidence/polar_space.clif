
(cl-text http://colore.oor.net/biparite_incidence/polar_space.clif

(cl-imports http://colore.oor.net/biparite_incidence/near_linear.clif)

(cl-imports http://colore.oor.net/biparite_incidence/gamma_space.clif)

(forall (p1 l1)
	(if	(and	(point p1)
			(line l1)
			(not (in p1 l1)))
		(exists (p2 l2)
			(and	(point p2)
				(line l2)
				(in p2 l1)
				(in p2 l2)
				(in p1 l2)))))

)
