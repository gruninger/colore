(cl-text ortho_modular_lattice

(cl-imports ortho_complemented_lattice)

(cl-comment "Orthomodular lattice axioms")

(cl-comment "OM: Orthomodular law")

(forall (x)
	(if
		(leq x y)
		(= y (join x (meet y (oc x))))))

)
