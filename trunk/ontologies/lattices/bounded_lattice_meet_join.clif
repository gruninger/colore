(cl-text http://colore.oor.net/lattices/bounded_lattice_meet_join

(cl-imports http://colore.oor.net/lattices/lattice_meet_join)

(cl-comment "Bounded lattice axioms expressed in terms of meet and join")

(cl-comment "L5join: Bottom (zero)")

(forall (x)
	(= (join zero x) x)
)


(cl-comment "L6join: Top (one)")

(forall (x)
	(= (join one x) one)
)

)
