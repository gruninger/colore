
(cl-text http://colore.oor.net/subposet_foliation/hereditary_subposet_foliation

(cl-imports http://colore.oor.net/subposet_foliation/subposet_foliation_root)

(forall (x y p1 p2)
        (if     (part p2 p1)
		(iff	(tpart x y p1)
                	(tpart x y p2))))

(forall (x y p1 p2)
        (if     (part p2 p1)
		(iff	(tlt x y p1)
                	(tlt x y p2))))

)
