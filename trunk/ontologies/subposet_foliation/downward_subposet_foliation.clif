(forall (x y p1 p2)
        (if     (and    (tleq x y p1)
                        (part p2 p1))
                (tleq x y p2)))

(forall (x y p1 p2)
        (if     (and    (tpart x y p1)
                        (part p2 p1))
                (tpart x y p2)))

)
