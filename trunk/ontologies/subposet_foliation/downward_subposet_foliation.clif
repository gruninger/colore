
(cl-text http://colore.oor.net/subposet_foliation/downward_subposet_foliation

(cl-imports http://colore.oor.net/subposet_foliation/subposet_foliation_root)

(forall (x y p1 p2)
        (if     (and    (tlt x y p1)
                        (part p2 p1))
                (tlt x y p2)))

(forall (x y p1 p2)
        (if     (and    (tpart x y p1)
                        (part p2 p1))
                (tpart x y p2)))

)
