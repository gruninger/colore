
(cl-text partial_down_subposet_foliation

(cl-imports subposet_foliation_root)

(forall (x y p1)
	(iff	(tlt x y p1)
		(forall (p2)
			(if	(part p2 p1)
				(tlt x y p2)))))

(forall (x y p1 p2)
        (if     (and    (tpart x y p1)
                        (part p2 p1))
                (tpart x y p2)))

)
