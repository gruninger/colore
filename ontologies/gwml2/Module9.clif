(cl-text http://colore.oor.net/gwml2/module9.clif
(cl-imports http://colore.oor.net/gwml2/module5.clif)

(cl-comment 'W-A1')
(cl-comment 'Well specializes Hydro Rock Body but is disjoint from Hydrogeo Unit')
(forall (x)
	(if
		(W x)
		(and
			(HRB x)
			(not (HGU x))
		)
	)
)

)