(cl-text http://colore.oor.net/orderings/definitions/glb.clif

(cl-imports http://colore.oor.net/orderings/quasiorder.clif)

(forall (x y s) 
	(iff	(glb x y s)
		(and	(leq s x)
			(leq s y)
			(forall (z)
				(if	(and	(leq z x)
						(leq z y))
					(leq z s))))))


)
