(cl-text http://colore.oor.net/sophos/sophos.clif

(forall (x r1 r2)
	(if	(and	(part r2 r1)
			(occupies x r1))
		(exists (y)
			(and	(physical_part y x)
				(occupies y r2)))))

(forall (x r1 r2)
	(if	(and	(C r2 r1)
			(occupies x r1))
		(exists (y)
			(and	(physical_C y x)
				(occupies y r2)))))

)
