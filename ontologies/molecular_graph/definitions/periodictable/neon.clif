(cl-text http://colore.oor.net/molecular_graph/definitions/periodictableneon.clif

(cl-comment 'Element name: neon')

(cl-comment 'Valence number: 0')

(cl-comment 'Axiom: E-2L')
(cl-comment 'Symbol: Ne')
(cl-comment 'Atomic Number: 2')
(cl-comment 'Neon has at least 0 covalent bonds.')
(forall (x) 
	(if (neon x)
		(atom x)))

(cl-comment 'Axiom: E-2M')
(cl-comment 'Symbol: Ne')
(cl-comment 'Atomic Number: 2')
(cl-comment 'Neon has at most 0 covalent bonds.')
(forall (x) 
	(if (neon x)
		(atom x)))


)