
(cl-text http://colore.oor.net/biparite_incidence/unique_line.clif

(cl-imports http://colore.oor.net/biparite_incidence/weak_bipartite.clif)

(forall (p1 p2 l1 l2)
	(if	(and	(point p1)
			(point p2)
			(line l1)
			(line l2)
			(in p1 l1)
			(in p1 l2)
			(in p2 l1)
			(in p2 l2))
		(or	(= p1 p2)
			(= l1 l2))))

)
