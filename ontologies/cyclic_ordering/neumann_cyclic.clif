
(cl-text http://colore.oor.net/cyclic_ordering/neumann_cyclic

(forall (x y z)
	(if	(C x y z)
		(C y z x)))

(forall (x y z)
	(iff	(and	(C x y z)
			(C y x z))
		(or	(= x y)
			(= x z)
			(= y z))))

(forall (x y z w)
	(if	(C x y z)
		(forall (w)
			(or	(C x y w)
				(C w y z)))))

(forall (x y z)
	(or	(C x y z)
		(C y x z)))

)
