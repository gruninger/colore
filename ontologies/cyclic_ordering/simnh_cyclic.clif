(forall (x y z)
	(if	(and	(not (= x y))
			(not (= x z))
			(not (= y z))
			(C x y z))
		(C y z x)))

(forall (x y z)
	(if	(and	(not (= x y))
			(not (= x z))
			(not (= y z))
			(C x y z))
		(not (C x z y))))

(forall (x y z)
	(if	(and	(not (= x y))
			(not (= x z))
			(not (= y z))
			(C x y z))
		(forall (w)
			(or	(C x y w)
				(C w y z)))))

(exists (x y z)
	(C x y z))

