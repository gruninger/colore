(cl-text http://colore.oor.net/constitution/theorems/constitution2mapping_preserve/Delta7.clif

(cl-imports http://colore.oor.net/constitution/constitution.clif)

(forall (x)
	(iff	(point x)
		(mat x)))

(forall (x)
	(iff	(line x)
		(MaterialObject x)))

(forall (x y)
	(iff	(part x y)
		(chunkOf x y)))

(forall (x y)
	(iff	(leq x y)
		(portionOf x y)))

(forall (x y)
	(iff	(in x y)
		(or	(constitutes x y)
			(constitutes y x)
			(= x y))))

)
