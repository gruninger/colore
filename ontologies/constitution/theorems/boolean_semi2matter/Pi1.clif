(cl-text http://colore.oor.net/constitution/theorems/boolean_semi2matter/Pi1.clif

(cl-imports http://colore.oor.net/mereology/m_mereology.clif)

(forall (x)
	(iff	(mat x)
		(= x x)))

(forall (x)
	(iff	(chunkOf x y)
		(part x y)))

)
