(cl-text http://colore.oor.net/poset_foliation/upward_p_foliation.clif

(cl-imports http://colore.oor.net/poset_foliation/p_foliation.clif)

(forall (x y p1 p2)
	(if	(and	(tleq x y p2)
			(part p2 p1))
		(tleq x y p1)))

)
