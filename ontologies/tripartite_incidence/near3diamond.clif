
(cl-text http://colore.oor.net/tripartite_incidence/near3diamond

(cl-imports http://colore.oor.net/tripartite_incidence/tripartite)

(forall (p q l1 l2 l3)
	(if	(and	(plane q)
			(point p)
			(in p q)
			(line l1)
			(line l2)
			(line l3)
			(in p l1)
			(in p l2)
			(in p l3)
			(in l1 q)
			(in l2 q)
			(in l3 q))
		(or	(= l1 l2)
			(= l1 l3)
			(= l2 l3))))

)
