(cl-text http://colore.oor.net/tripartite_incidence/disjoint_planes.clif

(cl-imports http://colore.oor.net/tripartite_incidence/weak_tripartite.clif)

(forall (q1 q2 x)
	(if	(and	(plane q1)
			(plane q2)
			(in x q1)
			(in x q2))
		(= q1 q2)))

)
