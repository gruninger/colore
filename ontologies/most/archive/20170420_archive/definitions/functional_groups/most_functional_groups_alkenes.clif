	(if (butene x)
		(or (1_butene x)(z_but_2_ene x)(e_but_2_ene x)(2_methylpropene x))))

(forall (x)
	(iff (or (1_butene x)(ethylethylene x))
		(but_1_ene x)))

(forall (x)
	(iff (but_1_ene x)
