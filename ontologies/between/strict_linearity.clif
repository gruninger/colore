(cl-text http://colore.oor.net/between/strict_linearity.clif

(cl-imports http://colore.oor.net/between/betweenness.clif)

(forall (x y z)
        (if     (and    (not (= x y))
                        (not (= x z))
                        (not (= y z)))
		(or	(between x y z)
			(between y z x)
			(between z x y))))

)