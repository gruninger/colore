(cl-text http://colore.oor.net/between/ps_lattice.clif

(forall (x y z)
	(if	(between x y z)
		(between z y x)))

(forall (x y z)
	(iff	(and	(between x y z)
			(between x z y))
		(= y z)))

(forall (a b c d x)
	(if	(and	(between a b c)
			(between a d b)
			(between a c x))
		(between d c x)))

)
