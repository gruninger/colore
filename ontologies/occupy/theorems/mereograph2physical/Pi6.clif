(cl-text http://colore.oor.net/occupy/theorems/mereograph2physical/Pi6.clif

(cl-imports http://colore.oor.net/mereograph/mereograph.clif)

(forall (x)
	(iff	(physical_object x)
		(= x x)))

(forall (x y)
	(iff	(physical_part x y)
		(leq x y)))

(forall (x y)
	(iff	(physical_C x y)
		(adj x y)))

)
