formulas(sos).
(all x all y  (in(x,y) -> ((line(x) | point(x)) & (line(y) | point(y))))).
(all x all y  (adj(x,y) -> ((line(x) | point(x)) & (line(y) | point(y))))).
(all x all y  ((point(x) & adj(x,y)) -> point(y))).
(all x all y  ((line(x) & adj(x,y)) -> line(y))).
(all x all y  ((in(x,y) & adj(x,y)) -> =(x,y))).
end_of_list.
